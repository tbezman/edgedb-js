{
  "compilerOptions": {
    "declarationDir": "./dist",
    "outDir": "./dist",
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "CommonJS",
    "jsx": "react",
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true
  },
  "include": ["src", "dbschema"]
}
